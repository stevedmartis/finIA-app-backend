    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.0.7",
    "twilio": "^5.0.3",
    "uuid": "^8.3.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.8",
    "@types/bcrypt": "^3.0.0",
    "@types/cryptr": "^4.0.3",
    "@types/dotenv": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.14",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@types/validator": "^13.11.6",
    "eslint": "^7.32.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
